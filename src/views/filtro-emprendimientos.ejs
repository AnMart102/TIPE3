    <!-- Custom CSS -->

    <link rel="stylesheet" href="/css/filtrar-emprendimientos.css" type="text/css">
    <title>Filtrar emprendimiento</title>
  </head>

  <body>
    <nav class="nav-vertical" id="menu_side">
      <div id="div-menu-icono">
        <i id="menu_icono" class="fas fa-bars"></i>
      </div>

      <div id="opciones-menu">
        <a href="#">
          <div class="opcion">
            <i class="fa-solid fa-chart-line"></i>
            <h4>Anal√≠tica</h4>
          </div>
        </a>

        <a href="">
          <div class="opcion">
            <i class="fa-solid fa-gear"></i>
            <h4>Editar perfil</h4>
          </div>
        </a>

        
      </div>
    </nav>
    <div id="contenido">
      <nav id="horizontal">
        <div id="usuario">
          <div id="usuario-texto">
            <div id="bloque-usuario-texto">
              <a id="Administrador" href="#">Administrador</a>
            </div>
            <div id="bloque-usuario-texto">
              <a id="Account" href="#">Account settings</a>
            </div>
          </div>
          <div id="foto-usuario">
            <div id="div-foto">
              
            </div>
          </div>
        </div>
      </nav>
      <div id="descripcion-pyme">
        <div id="fotos-perfil">
          <div id="imagen-principal">

          </div>
          <div id="imagenes-secundarias">
            <div class="imagen-secundaria">
            </div>
            <div class="imagen-secundaria">
            </div>
            <div class="imagen-secundaria">
            </div>
            <div class="imagen-secundaria">
            </div>

          </div>


        </div>
        <div id="descripcion-perfil">
          <div id="nombre-emprendimiento">
              <h2><%=formulario[0].Nombre_pyme%></h2>

          </div>
          <div id="categorias-emprendimiento">
            <div class="categoria">
              <p><%=formulario[0].categorias%></p>
            </div>
            <div class="categoria">
              <p><%=formulario[0].sub_cat%></p>
            </div>                    
          </div>
          <div id="datos-emprendimiento">
            <div class="dato">
              <ul>Direccion</ul>
              <ul>Fanpage de instagram</ul>                  
              <ul>Tienda virtual y/o fisica</ul>
              <ul>Correo</ul>
              <ul>numero de Telefono</ul>
            </div>
            <div class="dato">
              <ul><%= formulario[0].calle  %> <%= formulario[0].numero %></ul>
              <ul><%=formulario[0].Instagram%>aa</ul> 
              <ul><%=formulario[0].tipoTienda%></ul>
              <ul><%=formulario[0].Correo%></ul>
              <ul><%=formulario[0].Telefono%></ul>                                  
            </div>                                                                  
          </div>
          <div id="descripcion-emprendimiento">
            <p><%=formulario[0].Descripcion%></p>
          </div>
        </div>
      </div>
      <div id="descripcion-representante">
        <div class="container">
          <div class="row">
            <div class="col-3 col-datos-representante">
              <div class="datos-representante"><%=formulario[0].Nombre%></div>
            </div>
            <div class="col-3 col-datos-representante">
              <div class="datos-representante"><%=formulario[0].ApellidoP%></div>
            </div>
            <div class="col-3 col-datos-representante">
              <div class="datos-representante"><%=formulario[0].ApellidoM%></div>
            </div>
          </div>
          <div class="row">
            <div class="col-3 col-datos-representante">
              <div class="datos-representante">Antiguedad</div>
            </div>
            <div class="col-3 col-datos-representante">
              <div class="datos-representante">Empresa registrada: <%=formulario[0].Empresa_registrada%></div>
            </div>

            <!-- <div class="col-3 col-datos-representante">
              <div class="datos-representante"><%=formulario[0].Rut%></div>
            </div> -->
          </div>

          <div class="row">
            
          </div>

          <div class="row">
            <textarea class="comentarios-lectura" placeholder="Ingrese comentarios sobre la pyme."></textarea>
          </div>

          <div class="row">
            <div class="col-3 col-descargar-documentos">
              <div class="datos-representante"> <a href="#" download="">Descargar archivos adjuntos</a></div>
            </div>
            <div class="col-3 col-descargar-documentos">
              <div class="datos-representante"> <a href="#"> Abrir link de redes sociales</a></div>
            </div>
          </div>
        </div>
      </div>

      <div id="botones-filtro">
        <div class="boton-filtro">
          <form id="formulario" class="container-fluid" action="/botonaceptar" method="post">
              <input type="hidden"  name="Estado_sol" value=2>
              <input type="hidden"  name="Id_formulario" value='<%=formulario[0].id_formulario%>'>
              <input id="siguiente" type="submit" value='Aprovar' class="botones-moderador">
                <!-- <a href="#">registrarse</a> -->
          </form>
        </div>



        <div class="boton-filtro">
          <form id="formulario" class="container-fluid" action="/botonmodificar" method="post">
            <input type="hidden"  name="Estado_sol" value=4>
            <input type="hidden"  name="Id_formulario" value='<%=formulario[0].id_formulario%>'>
            <input id="siguiente" type="submit" value='Modificar' class="botones-moderador">
              <!-- <a href="#">registrarse</a> -->
        </form>
        </div>
        <div class="boton-filtro">
          <form id="formulario" class="container-fluid" action="/botonrechazar" method="post">
            <input type="hidden"  name="Estado_sol" value=3>
            <input type="hidden"  name="Id_formulario" value='<%=formulario[0].id_formulario%>'>
            <input id="siguiente" type="submit" value='Rechazar' class="botones-moderador">
              <!-- <a href="#">registrarse</a> -->
        </form>
        </div>
      </div>

    </div>
      
    <!-- Optional JavaScript -->
    <!-- jQuery first, then Popper.js, then Bootstrap JS -->
