<!doctype html>
<html lang="en">
  <head>
    <!-- Required meta tags -->
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1, shrink-to-fit=no">
    <script src="https://kit.fontawesome.com/41bcea2ae3.js" crossorigin="anonymous"></script>
    <script src="https://cdn.plot.ly/plotly-latest.min.js"></script>
    <!-- Bootstrap CSS -->
    <link rel="stylesheet" href="https://cdn.jsdelivr.net/npm/bootstrap@4.0.0/dist/css/bootstrap.min.css" integrity="sha384-Gn5384xqQ1aoWXA+058RXPxPg6fy4IWvTNh0E263XmFcJlSAwiGgFAW/dAiS6JXm" crossorigin="anonymous">
    <link rel="stylesheet" href="/css/estadisticasVisitas.css" type="text/css">
    <title>Estadísticas Administrador</title>
  </head>
  <body>

    <nav class="nav-vertical" id="menu_side">
      <div id="div-menu-icono">
        <i id="menu_icono" class="fas fa-bars"></i>
      </div>

      <div id="opciones-menu">
        <a href="#">
          <div class="opcion">
            <i class="fa-solid fa-chart-line"></i>
            <h4>Analítica</h4>
          </div>
        </a>

        <a href="">
          <div class="opcion">
            <i class="fa-solid fa-gear"></i>
            <h4>Editar perfil</h4>
          </div>
        </a>

        
      </div>
    </nav>
    <div id="contenido">
      <nav id="horizontal">
        <div id="usuario">
          <div id="usuario-texto">
            <div id="bloque-usuario-texto">
              <a id="Administrador" href="#">Administrador</a>
            </div>
            <div id="bloque-usuario-texto">
              <a id="Account" href="#">Account settings</a>
            </div>
          </div>
          <div id="foto-usuario">
            <div id="div-foto">
              
            </div>
          </div>
        </div>
      </nav>
      <div id="valores-visitas-pymes">
        <div class="nuevas">
          <h3>Total de pymes</h3>
          <h5>0</h5>
        </div>

        <div class="nuevas">
          <h3>Nuevas visitas</h3>
          <h5></h5>
        </div>        
      </div>

      <div id="grafico-visitas">
        <div id="descripcion-total-de-pymes">
          <h2>Total de pymes</h2>
        </div>
        <div id="grafico-nuevas-pymes">
          <div id="grafico-pymes" class="grafico"></div>
        </div>
      </div>
      <div id="solicitudes-pendientes">
        <h2>Solicitudes pendientes</h2>
      </div>

      <table id="tabla-visitas" class="table table-responsive table-hover">
        <thead>
            <tr>
                <th>Nombre pyme</th>
                <th>Nombre usuario</th>
                <th>Apellido P</th>
                <th>Apellido M</th>
                <th>Categorias</th>
                <th>Estado solicitud</th>
                <th></th>
                
            </tr>
        </thead>
        <tbody>
          <% for(var i = 0; i < datos3.length; i++){ %>
            <tr>
              <td><%= datos3[i].Nombre_pyme %></td>
              <td><%= datos3[i].Nombre %></td>
              <td><%= datos3[i].ApellidoP %></td>
              <td><%= datos3[i].ApellidoM %></td>
              <td><%= datos3[i].categorias %></td>
              <td><%= datos3[i].estado_sol %></td>
              <td><a id="Administrador" href="/admin/emprendimiento/<%= datos3[i].id_formulario %>">Revisar solicitud</a></td>
              
            </tr>
            <% } %>
        </tbody>
    </table>
    </div> 
    
    <!-- Optional JavaScript -->
    <!-- jQuery first, then Popper.js, then Bootstrap JS -->
    <script src="https://code.jquery.com/jquery-3.2.1.slim.min.js" integrity="sha384-KJ3o2DKtIkvYIK3UENzmM7KCkRr/rE9/Qpg6aAZGJwFDMVNA/GpGFF93hXpG5KkN" crossorigin="anonymous"></script>
    <script src="https://cdn.jsdelivr.net/npm/popper.js@1.12.9/dist/umd/popper.min.js" integrity="sha384-ApNbgh9B+Y1QKtv3Rn7W3mgPxhU9K/ScQsAP7hUibX39j7fakFPskvXusvfa0b4Q" crossorigin="anonymous"></script>
    <script src="/js/nav-vertical-move.js"></script>
    <script src="/js/graficos2.js"></script>
    <script src="https://cdn.jsdelivr.net/npm/bootstrap@4.0.0/dist/js/bootstrap.min.js" integrity="sha384-JZR6Spejh4U02d8jOt6vLEHfe/JQGiRRSQQxSfFWpi1MquVdAyjUar5+76PVCmYl" crossorigin="anonymous"></script>
  </body>
</html>