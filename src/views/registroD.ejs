<!doctype html>
<html lang="en">
  <head>
    <!-- Required meta tags -->
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1, shrink-to-fit=no">

    <!-- Bootstrap CSS -->
    <link rel="stylesheet" href="https://cdn.jsdelivr.net/npm/bootstrap@4.0.0/dist/css/bootstrap.min.css" integrity="sha384-Gn5384xqQ1aoWXA+058RXPxPg6fy4IWvTNh0E263XmFcJlSAwiGgFAW/dAiS6JXm" crossorigin="anonymous">
    <link rel="stylesheet" href="/css/registro.css" type="text/css">
    <title>Index</title>
  </head>
  <body>
    <div id="contenedor-pagina">
      <nav id="navbar">
          <div class="container-fluid">
            <div class="row">
              <div class="col-3 col-navbar">
                <H1>LOGO</H1>
              </div>
              <div class="col-2 col-navbar">
                <a href="/">inicio</a>
              </div>
              <div class="col-2 col-navbar">
                <a href="/">emprendimientos</a>
              </div>
              <div class="col-3 col-navbar">
                <a href="#">ya est√° registrado?</a>
              </div>
              <div class="col-2 col-navbar">
                <div id="iniciar-sesion-buttom">
                  <a href="/login">Iniciar Sesion</a>
                </div>
              </div>
            </div>
          </div>
      </nav>
      <div id="registro">
        <div id="titulo-registro">
          <h1>Registro</h1>
        </div>
        <div class="titulo-formulario">
          <h2>Emprendedor ingrese sus datos</h2>
        </div>
        <div class="titulo-formulario">
          <h2>Direccion</h2>
        </div>

        <form id="formulario" class="container-fluid" action="/registroD" method="post">

          <div class="row">
            <div class="col-3">
              <INPUT class="datos-formulario" type="text" name="calle" placeholder="Calle">
            </div>
            <div class="col-3">
              <INPUT class="datos-formulario" type="text" name="numeroC" placeholder="Numero">
            </div>
            <div class="col-3">
              <div class="form-group">
                <select  name="CasaDepto" required class="form-control"  placeholder="Probabilidad de ocurrencia del riesgo"> 
                  <option value="" disabled selected hidden>Casa/Depto</option>
                  <option value="1">Casa</option>
                  <option value="2">Depto</option>
                </select>
            </div>
            </div>
          </div>
          <div class="row">
            <div class="col-3">
              <INPUT class="datos-formulario" type="text" name="localidad" placeholder="Localidad">
            </div>
            <div class="col-3">
              <INPUT class="datos-formulario" type="text" name="PoblaVilla" placeholder="Poblacion/Villa">
            </div>
          </div>
          <input type="hidden"  name="Rut" value='<%=Info[0].Rut%>'>


          <div id="botones-enviar-formulario">
            <input id="siguiente" type="submit" class="btn btn-primary">
            <a id="siguiente" href="">Ayuda</a>
              <!-- <a href="#">registrarse</a> -->
          </div>
        </form>
      </div>

      </div>
    </div>
      
    <!-- Optional JavaScript -->
    <!-- jQuery first, then Popper.js, then Bootstrap JS -->
    <script src="https://code.jquery.com/jquery-3.2.1.slim.min.js" integrity="sha384-KJ3o2DKtIkvYIK3UENzmM7KCkRr/rE9/Qpg6aAZGJwFDMVNA/GpGFF93hXpG5KkN" crossorigin="anonymous"></script>
    <script src="https://cdn.jsdelivr.net/npm/popper.js@1.12.9/dist/umd/popper.min.js" integrity="sha384-ApNbgh9B+Y1QKtv3Rn7W3mgPxhU9K/ScQsAP7hUibX39j7fakFPskvXusvfa0b4Q" crossorigin="anonymous"></script>
    <script src="https://cdn.jsdelivr.net/npm/bootstrap@4.0.0/dist/js/bootstrap.min.js" integrity="sha384-JZR6Spejh4U02d8jOt6vLEHfe/JQGiRRSQQxSfFWpi1MquVdAyjUar5+76PVCmYl" crossorigin="anonymous"></script>
  </body>
</html>


